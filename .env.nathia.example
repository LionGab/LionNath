# NAT-IA Configuration Example
# Copy this file to .env and fill with your actual values

# ===========================
# Supabase Configuration
# ===========================
# Obtenha em: https://app.supabase.com/project/_/settings/api
EXPO_PUBLIC_SUPABASE_URL=https://seu-projeto.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# ===========================
# NAT-IA Configuration
# ===========================
# Timeout para requisições (em milissegundos)
EXPO_PUBLIC_NATHIA_TIMEOUT=5000

# Número máximo de tentativas em caso de erro
EXPO_PUBLIC_NATHIA_MAX_RETRIES=2

# Base URL das Edge Functions (geralmente não precisa mudar)
# EXPO_PUBLIC_NATHIA_BASE_URL=https://seu-projeto.supabase.co/functions/v1

# ===========================
# Analytics (opcional)
# ===========================
# EXPO_PUBLIC_ANALYTICS_KEY=your-analytics-key

# ===========================
# Feature Flags (opcional)
# ===========================
# Habilitar modo debug
# EXPO_PUBLIC_NATHIA_DEBUG=true

# Habilitar modo offline
# EXPO_PUBLIC_NATHIA_OFFLINE_MODE=false

# Habilitar voice input
# EXPO_PUBLIC_NATHIA_VOICE_INPUT=false

# ===========================
# Development Only
# ===========================
# Mock responses para desenvolvimento
# EXPO_PUBLIC_NATHIA_MOCK_MODE=false

# Log level (debug, info, warn, error)
# EXPO_PUBLIC_LOG_LEVEL=info
