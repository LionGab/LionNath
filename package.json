{
  "name": "nossa-maternidade",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "pnpm run validate:env && pnpm -C apps/mobile dev",
    "build": "pnpm run validate:env && turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "turbo run typecheck",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:watch": "turbo run test -- --watch",
    "test:coverage": "turbo run test -- --coverage",
    "test:ui": "turbo run test -- --ui",
    "test:nathia": "node scripts/test-nathia-functions.js",
    "test:nathia:chat": "node scripts/test-nathia-functions.js nathia-chat",
    "test:nathia:all": "node scripts/test-nathia-functions.js all",
    "e2e:android": "maestro test e2e/maestro/smoke-flow.yaml",
    "check": "pnpm run type-check && pnpm run lint && pnpm run test",
    "validate": "pnpm run validate:env && pnpm run type-check && pnpm run lint && pnpm run test && pnpm run format:check",
    "validate:full": "pnpm run validate && pnpm run test:coverage",
    "validate:quick": "pnpm run type-check && pnpm run lint",
    "validate:env": "node scripts/validate-env.js",
    "audit": "pnpm audit --audit-level=moderate",
    "prepare": "husky install",
    "agents:start": "node scripts/start-agents.js",
    "agents:start:windows": "powershell -ExecutionPolicy Bypass -File scripts/start-agents-windows.ps1",
    "agents:stop": "node scripts/stop-agents.js",
    "agents:status": "node scripts/agents-status.js",
    "agents:logs": "node scripts/agents-logs.js",
    "browser:automation": "tsx scripts/browser-automation-agent.ts",
    "refactor:web": "tsx scripts/refactor-web-agent.ts",
    "design:system:v1": "tsx scripts/design-system-v1-agent.ts",
    "metrics:backfill": "tsx scripts/backfill-metrics.ts",
    "metrics:weekly-report": "tsx scripts/generate-weekly-report.ts",
    "review:keep": "tsx scripts/review-keeper-agent.ts",
    "precommit": "lint-staged",
    "prepush": "pnpm run validate:quick"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.4.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "^5.1.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@sentry/react-native": "^7.5.0",
    "@stripe/stripe-react-native": "0.38.6",
    "@supabase/supabase-js": "^2.48.0",
    "@types/react": "~19.1.10",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "expo-av": "~16.0.7",
    "expo-notifications": "~0.32.12",
    "expo-status-bar": "~3.0.8",
    "glob": "^10.4.5",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.8",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "tsx": "^4.7.0",
    "turbo": "^2.1.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "@types/react": "~19.1.10",
      "react": "19.1.0",
      "react-native-safe-area-context": "~5.6.0"
    }
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.30.1",
    "expo-constants": "~18.0.10",
    "expo-linear-gradient": "~15.0.7",
    "tailwindcss-animate": "^1.0.7"
  }
}
